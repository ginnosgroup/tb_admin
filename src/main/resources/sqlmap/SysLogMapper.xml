<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zhinanzhen.b.dao.SysLogDAO">

    <insert id="insertLog" parameterType="org.zhinanzhen.b.dao.pojo.SysLogDO">
        INSERT INTO `b_sys_log`(`id`, `uri`,`daoMethodName`, `ip`, `wholeSql`,`desc`, `createDate`, `user_id`)
        VALUES (#{id}, #{uri}, #{daoMethodName}, #{ip}, #{wholeSql}, #{desc}, #{createDate}, #{userId})
    </insert>

</mapper>
